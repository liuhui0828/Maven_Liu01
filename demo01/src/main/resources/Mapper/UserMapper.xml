<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cn.demo01.Mapper.UserMapper" >
<resultMap id="resultMap" type="User">
    <result property="name" column="username"></result>
    <result property="psd"  column="password"></result>
</resultMap>
    <select id="getAllUser" resultMap="resultMap">
        select * from user
    </select>

    <select id="getUserByUsername" parameterType="String" resultMap="resultMap">
        SELECT * FROM USER WHERE username = #{username}

    </select>

    <insert id="insertUser" parameterType="com.cn.demo01.Pojo.User">
        insert into user(username,password) values (#{name},#{psd})
    </insert>

</mapper>